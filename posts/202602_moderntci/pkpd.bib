	keywords = {Hemodynamics, Aged, Female, Humans, Hypotension, Male, Middle Aged, Adult, Dose-Response Relationship, Drug, Blood Pressure, Heart Rate, Propofol, Anesthetics, Intravenous, Anesthesia, Piperidines, Pharmacology, Remifentanil, Analgesics, Opioid, General anaesthesia, Haemodynamics, Induction, Laryngoscopy},
	pages = {1347--1355},
	file = {Full Text:/home/jaj/Zotero/storage/A2GEVFB4/Marges et al. - 2024 - Comparing the haemodynamic effects of high- and low-dose opioid anaesthesia a secondary analysis of.pdf:application/pdf},
