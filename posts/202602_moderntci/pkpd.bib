	keywords = {Hemodynamics, Humans, Healthy Volunteers, Propofol, Anesthetics, Intravenous, Piperidines, propofol, haemodynamic, healthy volunteers, mechanism-based pharmacodynamic modelling, Remifentanil, interaction, remifentanil},
	pages = {222--233},
	file = {Full Text:/home/jaj/Zotero/storage/IA4GZTX2/Su et al. - 2023 - Pharmacodynamic mechanism-based interaction model for the haemodynamic effects of remifentanil and p.pdf:application/pdf},
