	number = {5},
	urldate = {2025-09-12},
	journal = {British Journal of Anaesthesia},
	author = {Van Den Berg, J.P. and Eleveld, D.J. and De Smet, T. and Van Den Heerik, A.V.M. and Van Amsterdam, K. and Lichtenbelt, B.J. and Scheeren, T.W.L. and Absalom, A.R. and Struys, M M R F},
	month = nov,
	year = {2017},
	pages = {918--927},
	file = {PDF:/home/jaj/Zotero/storage/MHGB8GP5/Van Den Berg et al. - 2017 - Influence of Bayesian optimization on the performance of propofol target-controlled infusion.pdf:application/pdf},
