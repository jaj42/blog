	keywords = {Models, Biological, Pharmacokinetics, physiological models, 3-compartment pharmacokinetic model, analytical solution, biexponential functions, biocontrol, Committee on Communications and Information Policy, computer-controlled infusion pumps, computerised control, Difference equations, Differential equations, drug disposition, Drugs, Helium, Infusion Pumps, intravenous bolus injection, intravenous drugs titration, numerical techniques, patient treatment, Plasma applications, plasma drug concentrations, Plasma stability, polyexponential disposition functions, Therapy, Computer-Assisted, triexponential functions},
	pages = {522--525},
	file = {Bailey_Shafer_1991_A simple analytical solution to the three-compartment pharmacokinetic model.pdf:/home/jaj/Zotero/storage/U5YLQBNC/Bailey_Shafer_1991_A simple analytical solution to the three-compartment pharmacokinetic model.pdf:application/pdf;IEEE Xplore Abstract Record:/home/jaj/Zotero/storage/ZP46MV9F/articleDetails.html:text/html},
