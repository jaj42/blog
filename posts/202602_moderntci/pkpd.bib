	pages = {13579},
	file = {Full Text:/home/jaj/Zotero/storage/MGJCMMIC/Kroneisl et al. - 2023 - Peri-Operative Kinetics of Plasma Mitochondrial DNA Levels during Living Donor Kidney Transplantatio.pdf:application/pdf},
