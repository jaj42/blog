	author = {Li, Yingxue and Koomen, Jeroen V. and Eleveld, Douglas J. and van den Berg, Johannes P. and Vos, Jaap Jan and de Keijzer, Ilonka N. and Struys, Michel M. R. F. and Colin, Pieter J.},
	month = nov,
	year = {2024},
	keywords = {Norepinephrine, Female, Humans, Male, Middle Aged, Adult, Models, Biological, Dose-Response Relationship, Drug, Cross-Over Studies, Healthy Volunteers, Propofol, Anesthesia, General, Anesthetics, Intravenous, Young Adult, Piperidines, Remifentanil},
