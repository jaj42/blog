	number = {3},
	journal = {European Journal of Neurology},
	author = {Maïer, Benjamin and Gory, Benjamin and Lapergue, Bertrand and Sibon, Igor and Escalard, Simon and Kyheng, Maeva and Labreuche, Julien and de Havenon, Adam and Petersen, Nils and Anadani, Mohammad and Gayat, Etienne and Boursin, Perrine and Ben Maacha, Malek and Desilles, Jean-Philippe and Blanc, Raphael and Piotin, Michel and Halimi, Jean-Michel and Mazighi, Mikael and {BP TARGET Investigators}},
